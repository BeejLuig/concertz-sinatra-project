<div class="user">
  <h1>Your Account</h1>
  <% @user.artists.each do |artist| %>
    <p><%= artist.name %> - <a href="/artists/<%= artist.slug %>">View</a> - <a href="/artists/<%= artist.slug %>/edit">Edit</a></p>
  <% end %>
  <p><a href="/users/<%=@user.id%>/edit">Edit your account</a></p>
</div>
